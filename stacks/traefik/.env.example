# Traefik Configuration
# Copy to .env and customize

TRAEFIK_NETWORK=traefik
HTTP_PORT=80
HTTPS_PORT=443
DASHBOARD_PORT=8080

TRAEFIK_DOMAIN=traefik.localhost
TRAEFIK_DASHBOARD_INSECURE=true

LOG_LEVEL=INFO

# Let's Encrypt - set ACME_EMAIL for SSL
ACME_EMAIL=
ACME_SERVER=https://acme-v02.api.letsencrypt.org/directory

# Cloudflare DNS Challenge - set in .env (never commit .env)
# CF_DNS_API_TOKEN=your-dns-api-token
